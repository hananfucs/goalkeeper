<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hf.goalkeeper">

    <application
        android:name=".core.GoalKeeperApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".viewes.activities.PlayersListActivity"/>



        <activity android:name=".viewes.activities.GameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".viewes.activities.GamePlayActivity"/>
        <activity android:name=".viewes.activities.GameSettingsActivity" android:exported="true"/>

    </application>

</manifest>